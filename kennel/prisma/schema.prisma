generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Kennel {
  id         String   @id @default(uuid())
  name       String
  address    String
  productsId String[]
  vets       String[]
  animals    String[]
}

model Product {
  id    String @id @default(uuid())
  name  String
  price Float
  stock Int
}
